    "suno",
    "elevenlabs",
    "bytedance",
    "qwen",
    "runway",
    "midjourney",
    "wan",
    "openai-4o-image",
    "flux-kontext",
    "recraft",
    "ideogram",
    "model-context-protocol",
    "ai-api",
    "unified-ai"
  ],
  "author": "felores",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felores/kie-ai-mcp-server.git"
  },
  "homepage": "https://github.com/felores/kie-ai-mcp-server#readme",
  "bugs": {
    "url": "https://github.com/felores/kie-ai-mcp-server/issues"
