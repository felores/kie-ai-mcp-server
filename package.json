{
  "name": "@felores/kie-ai-mcp-server",
  "version": "1.9.6",
  "description": "MCP server for Kie.ai APIs (20 AI tools: Nano Banana image generation/editing/upscaling, Veo3 video generation, Suno music generation, ElevenLabs TTS/sound effects, ByteDance Seedance/Seedream video/image, Qwen image generation/editing, Runway Aleph video transformation, Midjourney AI generation, Wan video generation, Kling video generation, OpenAI 4o image generation/editing/variants, Flux Kontext image generation/editing, Recraft background removal, Ideogram image reframing)",
  "main": "dist/index.js",
  "bin": {
    "kie-ai-mcp-server": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --env-file=.env dist/index.js",
    "dev": "tsx --env-file=.env src/index.ts",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "kie.ai",
    "ai",
    "image-generation",
    "video-generation",
    "music-generation",
    "text-to-speech",
